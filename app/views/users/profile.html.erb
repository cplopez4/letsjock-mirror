  <div class="profileInfo relative">
    <h1><%=	@user.name  %> <%=	@user.lastname  %></h1>
    <p><strong>Soccer Goalkeeper</strong> <%  if @user.birth %>/ <%=  ((Time.new - @user.birth)/1.year).ceil - 1 %> years<%  end %><%  if @user.citybirth != "" %> ,<%=  @user.citybirth %>, <%  end %><%=  @user.country %></p>
    <ul class="boxConnect">
      <li><a class="verified" href="#"></a></li>
      <li><a class="connect" href="#"><p>connect</p></a></li>
      <li><a class="jockOn" href="#"><p>jock-on!</p></a></li>
    </ul><!-- /boxConect -->
  </div><!-- /profileInfo -->

  <div class="experienceLine">
    <ul>
      <li>
        <h2>Abstract</h2>
        <p><%=	 @user.resume %></p>
        <% if signed_in? && current_user.id == @user.id %>
            <%=	 form_for @user  do |f|%>
            Birth <%=  f.date_select :birth %>
            City Birth <%=  f.text_field :citybirth  %>
            Country <%=	 f.text_field :country %>
            Resume <%=	 f.text_field :resume %>
            <%=	 f.submit %>
            <%	 end %>
        <%	 end %>
      </li>

      <li>
        <h2>Latest honors</h2>
        <%	if @honors.any?  %>
            <%=	 render @honors %>
        <%	end %>
        <% if signed_in? && current_user.id == @user.id %>
            <%= form_for(@result, :url => { :controller => :results, :action => :create }) do |f| %>
                <%= f.number_field :value %>
                <%= f.text_field :var %>
                <%= f.number_field :sport_id %>
                <%= f.number_field :competition_id %>
                <%= f.number_field :team_id %>
                <%= f.text_field :description %>
                <%= f.date_select :date %>
                <%= f.hidden_field :user_id, :value => @user.id %>
                <%= f.submit "Create Result" %>
            <% end %>
            <%= form_for(@recognition, :url => { :controller => :recognitions, :action => :create }) do |f| %>
                <%= f.number_field :sport_id %>
                <%= f.number_field :competition_id %>
                <%= f.number_field :team_id %>
                <%= f.text_field :description %>
                <%= f.date_select :date %>
                <%= f.hidden_field :user_id, :value => @user.id %>
                <%= f.submit "Create Recognition" %>
            <% end %>
         <%	 end %>
      </li>

      <li>
        <h2>Experience</h2>
        <%	if @experiences.any?  %>
            <%=	 render @experiences %>
        <%	end %>
        <% if signed_in? && current_user.id == @user.id %>
            <%= form_for(@competition, :url => { :controller => :competitions, :action => :create }) do |f| %>
                <%= f.text_field :name %>
                <%= f.number_field :sport_id %>
                <%= f.date_select :init %>
                <%= f.date_select :end %>
                <%= f.hidden_field :user_id, :value => @user.id %>
                <%= f.submit "Create Competition" %>
            <% end %>
            <%= form_for(@team, :url => { :controller => :teams, :action => :create }) do |f| %>
                <%= f.text_field :name %>
                <%= f.number_field :sport_id %>
                <%= f.date_select :init %>
                <%= f.date_select :end %>
                <%= f.hidden_field :user_id, :value => @user.id %>
                <%= f.submit "Create Team" %>
            <% end %>
            <%= form_for(@train, :url => { :controller => :trains, :action => :create }) do |f| %>
                <%= f.text_field :name %>
                <%= f.number_field :sport_id %>
                <%= f.number_field :team_id %>
                <%= f.date_select :init %>
                <%= f.date_select :end %>
                <%= f.hidden_field :user_id, :value => @user.id %>
                <%= f.submit "Create Train" %>
            <% end %>
        <% end %>
      </li>

    </ul>
  </div><!-- /experienceLine -->

